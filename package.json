{
  "name": "audacity-nextjs-template",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "react-map-gl": "5.2.12"
  },
  "scripts": {
    "build": "npm run partytown && npm run audacity && next build",
    "dev": "npm run partytown && npm run audacity && next dev",
    "start": "next start",
    "lint": "next lint",
    "partytown": "partytown copylib ./public/~partytown",
    "audacity": "mkdir -p public/~audacity && cp node_modules/@retailhub/audacity/build/src/app.css public/~audacity/app.css"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@retailhub/audacity": "^1.0.63",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.16",
    "next": "14.0.3",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
